<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:id="@+id/absurd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_margin="10dp"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="Proprietor:"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_margin="16dp"
            android:text="yash Khandelwal"
            android:textStyle="bold"
            android:textSize="16sp"
            android:layout_toRightOf="@id/absurd"
            android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/pemail1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_margin="10dp"
        android:layout_below="@id/absurd"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="Email:"/>
    <TextView
        android:id="@+id/pemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/pemail1"
        android:layout_margin="16dp"
        android:textStyle="bold"
        android:textSize="16sp"
        android:text="yashkhandelwal99299@gmail.com"
        android:layout_below="@id/absurd"

        />
</RelativeLayout>


